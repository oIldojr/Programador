class ContaBancaria: 
    def __init__(self,titular,saldo):
        self.titular = titular
        self.__saldo = saldo
    def depositar(self,valor):
        if valor > 0:
            self.__saldo += valor
    def exibir_info(self):
        print(f"Saldo atualizado{self.__saldo}")

conta1 = ContaBancari("Ildo", 10)
conta1.depositar(100)
conta1.exibir_info()

